<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.slawomirmakurat.ekoagromarket.drawer.Profile_user"
    tools:showIn="@layout/activity_profile_user">


    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteY="16dp"
        tools:layout_editor_absoluteX="16dp">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="56dp"
            android:layout_height="56dp"
            app:srcCompat="@drawable/avatar_default"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="@+id/textView16"
            android:layout_marginStart="8dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="8dp" />

        <TextView
            android:id="@+id/Login_User_TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Imię_Login"
            android:layout_marginStart="8dp"
            app:layout_constraintLeft_toRightOf="@+id/imageView"
            android:layout_marginLeft="8dp"
            app:layout_constraintTop_toTopOf="@+id/imageView" />

        <ImageView
            android:id="@+id/Certyfikat"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@android:drawable/btn_star_big_on"
            app:layout_constraintBottom_toBottomOf="@+id/imageView"
            android:layout_marginBottom="8dp"
            app:layout_constraintTop_toTopOf="@+id/imageView"
            android:layout_marginTop="8dp"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent" />

        <TextView
            android:id="@+id/km_TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="km"
            app:layout_constraintLeft_toRightOf="@+id/dystance_TextView"
            android:layout_marginLeft="4dp"
            app:layout_constraintBottom_toBottomOf="@+id/dystance_TextView"
            android:layout_marginStart="4dp" />

        <TextView
            android:id="@+id/City_TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Kartuzy"
            android:layout_marginLeft="32dp"
            app:layout_constraintLeft_toLeftOf="@+id/textView13"
            app:layout_constraintBottom_toBottomOf="@+id/textView13"
            android:layout_marginStart="32dp" />

        <TextView
            android:id="@+id/dystance_TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="20"
            android:layout_marginStart="11dp"
            app:layout_constraintLeft_toRightOf="@+id/City_TextView"
            android:layout_marginLeft="11dp"
            app:layout_constraintBottom_toBottomOf="@+id/City_TextView" />

        <TextView
            android:id="@+id/textView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="..."
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/Login_User_TextView"
            app:layout_constraintRight_toLeftOf="@+id/imageButton"
            app:layout_constraintLeft_toRightOf="@+id/imageView"
            android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" />

        <ImageButton
            android:id="@+id/imageButton"
            android:layout_width="15dp"
            android:layout_height="20dp"
            android:layout_marginLeft="24dp"
            android:layout_marginTop="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/textView13"
            app:layout_constraintLeft_toRightOf="@+id/imageView"
            app:layout_constraintVertical_bias="1.0"
            app:srcCompat="@drawable/ic_place_black_24dp"
            app:layout_constraintTop_toBottomOf="@+id/Login_User_TextView"
            android:layout_marginStart="24dp" />

        <TextView
            android:id="@+id/textView16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="Krótki opis jakie mam gospodarstwo co robię czym się zajmuję i jaki jestem fajny rolnik bo uprawiam Eko Zywność, Krótki opis jakie mam gospodarstwo co robię czym się zajmuję i jaki jestem fajny rolnik bo uprawiam Eko Zywność  "
            app:layout_constraintTop_toBottomOf="@+id/imageView"
            tools:layout_editor_absoluteX="0dp" />

        <TextView
            android:id="@+id/UserAdd_TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:text="Dołączył"
            app:layout_constraintLeft_toRightOf="@+id/Login_User_TextView"
            app:layout_constraintTop_toTopOf="@+id/Login_User_TextView"
            android:layout_marginStart="30dp" />

        <TextView
            android:id="@+id/date_TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:text="12-04-2017"
            app:layout_constraintLeft_toRightOf="@+id/UserAdd_TextView"
            app:layout_constraintTop_toTopOf="@+id/UserAdd_TextView"
            android:layout_marginStart="8dp" />

        <TextView
            android:id="@+id/textView19"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="Moje produkty"
            app:layout_constraintLeft_toLeftOf="@+id/textView16"
            app:layout_constraintTop_toBottomOf="@+id/textView16" />

